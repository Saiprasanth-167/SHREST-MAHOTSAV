{
  "functions": {
    "api/**/*.js": { "runtime": "nodejs18.x" }
  },
  "routes": [
    { "src": "/api/register", "dest": "/api/register.js" },
    { "src": "/api/validate-utr", "dest": "/api/validate-utr.js" },
    { "src": "/api/upi-qr", "dest": "/api/upi-qr.js" },
    { "src": "/api/upi-config", "dest": "/api/upi-config.js" },
    { "src": "/api/send-otp", "dest": "/api/send-otp.js" },
    { "src": "/api/verify-otp", "dest": "/api/verify-otp.js" },
    { "src": "/api/live-excel", "dest": "/api/live-excel.js" },
    { "src": "/api/download-excel", "dest": "/api/download-excel.js" },
    { "src": "/api/registrations/(.*)", "dest": "/api/registrations/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}